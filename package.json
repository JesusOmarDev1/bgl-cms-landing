{
  "name": "bgl-cms",
  "version": "1.0.0",
  "description": "Sistema de gestión de contenidos (CMS) corporativo desarrollado específicamente para BGL BASCULAS INDUSTRIALES, empresa líder en soluciones de pesaje industrial y básculas de alta precisión.",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:turbo": "cross-env NODE_OPTIONS=--no-deprecation next build --turbopack",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:turbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build --turbopack && pnpm start ",
    "generate:importmap": "cross-env NODE_OPTIONS=\"--no-deprecation --import ./node-css-loader.mjs\" payload generate:importmap",
    "generate:importmap:alt": "cross-env NODE_OPTIONS=--no-deprecation tsx --ignore-css node_modules/payload/bin/payload.js generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=\"--no-deprecation --import ./node-css-loader.mjs\" payload generate:types",
    "generate:types:alt": "cross-env NODE_OPTIONS=--no-deprecation tsx --ignore-css node_modules/payload/bin/payload.js generate:types",
    "generate:all": "cross-env NODE_OPTIONS=\"--no-deprecation --loader ./node-css-loader.mjs\" payload generate:types && cross-env NODE_OPTIONS=\"--no-deprecation --loader ./node-css-loader.mjs\" payload generate:importmap",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "clean:cache": "cross-env NODE_OPTIONS=--no-deprecation rimraf .next",
    "format:check": "cross-env NODE_OPTIONS=--no-deprecation prettier --check",
    "format": "cross-env NODE_OPTIONS=--no-deprecation prettier --write",
    "delete:localdb": "cross-env NODE_OPTIONS=--no-deprecation rimraf bgl-cms.db"
  },
